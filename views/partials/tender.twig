{% for i in 0 .. tender|length - 1 %}
<div class="pt-5 border-bottom">
    <h5 class="text-sea-buckthorn text-left">{{tender[i]['title']}}</h5>
    <ul class="list-group list-group-flush text-left">
        <li class="list-group-item bg-wild-sand">
            <ul class="list-group">
                <li class="bg-wild-sand border-0 list-group-item"><strong>Tender No:</strong> {{tender[i]['tenderNo']}}</li>
                <li class="bg-wild-sand border-0 list-group-item"><strong>Date of Tender:</strong> {{tender[i]['date']}}</li>
                <li class="bg-wild-sand border-0 list-group-item"><strong>Last Date & Time of Tender:</strong> {{tender[i]['lastDate']}}</li>
                <a href="{{tender[i]['noticeURL']}}" class="text-reset">
                    <li class="bg-wild-sand list-group-item">Download Notice</li>
                </a>
            </ul>
        </li>
    </ul>
</div>
{% endfor %}